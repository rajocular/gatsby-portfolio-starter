{
  "name": "portfolio-starter",
  "private": true,
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "env-cmd gatsby build",
    "develop": "env-cmd gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "deploy": "env-cmd -f .env gatsby build && aws s3 cp public/. s3://gatsby-portfolio-1 --recursive --profile command-line-access"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "13.4.0",
    "@material-ui/core": "4.4.0",
    "@material-ui/icons": "4.2.1",
    "classnames": "2.2.6",
    "gatsby": "4.25.7",
    "gatsby-source-contentful": "2.1.30",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-lottie": "1.2.3"
  },
  "devDependencies": {
    "env-cmd": "10.0.1",
    "prettier": "1.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
